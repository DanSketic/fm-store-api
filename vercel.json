{
  "version": 2,
  "installCommand": "npm install",
  "outputDirectory": ".",
  "framework": null,
  "devCommand": "nodemon index.js",
  "buildCommand": "gulp",
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["data/**/*","dist/**","routes/**","models/**"] }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "index.js"
      }
  ]
}
